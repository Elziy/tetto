<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.elite.tetto.recommend.dao.HistoryDao">
	
	<!-- 可根据自己的需求，是否要使用 -->
	<resultMap type="com.elite.tetto.recommend.entity.HistoryEntity" id="historyMap">
		<result property="id" column="id"/>
		<result property="uid" column="uid"/>
		<result property="aid" column="aid"/>
		<result property="browseTime" column="browse_time"/>
	</resultMap>
	<insert id="addHistory">
		INSERT INTO rec_history
		VALUES (NULL, #{uid}, #{aid}, #{browseTime})
		on DUPLICATE KEY
			UPDATE browse_time = #{browseTime}
	</insert>


</mapper>